{
  "projectType": "application",
  "sourceRoot": "apps/kujali-functions/src",
  "prefix": "app",
  "targets": {
    "build": {
      "executor": "@angular-devkit/build-angular:browser",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/kujali-functions",
        "index": "apps/kujali-functions/src/index.html",
        "main": "apps/kujali-functions/src/main.ts",
        "polyfills": "apps/kujali-functions/src/polyfills.ts",
        "tsConfig": "apps/kujali-functions/tsconfig.app.json",
        "inlineStyleLanguage": "scss",
        "assets": ["apps/kujali-functions/src/assets"],
        "scripts": [],
        "allowedCommonJsDependencies": ["firebase-admin", "firebase-functions"]
      },
      "configurations": {
        "production": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "1mb",
              "maximumError": "4mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "2kb",
              "maximumError": "4kb"
            }
          ],
          "fileReplacements": [
            {
              "replace": "apps/kujali-functions/src/environments/environment.ts",
              "with": "apps/kujali-functions/src/environments/environment.prod.ts"
            }
          ],
          "outputHashing": "all"
        },
        "development": {
          "buildOptimizer": false,
          "optimization": false,
          "vendorChunk": true,
          "extractLicenses": false,
          "sourceMap": true,
          "namedChunks": true
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "executor": "@angular-devkit/build-angular:dev-server",
      "configurations": {
        "production": {
          "browserTarget": "kujali-functions:build:production"
        },
        "development": {
          "browserTarget": "kujali-functions:build:development"
        }
      },
      "defaultConfiguration": "development"
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "browserTarget": "kujali-functions:build"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "apps/kujali-functions/**/*.ts",
          "apps/kujali-functions/**/*.html"
        ]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/apps/kujali-functions"],
      "options": {
        "jestConfig": "apps/kujali-functions/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
